TARGET2   =  O3-stat

OBJECTS2   = O3-stat.f90

# F_FLAGS   = -O -FR -no-wrap-margin -qopenmp
F_FLAGS   = -no-wrap-margin -qopenmp

FC        = ftn 

LIBS      = ${WGRIB2_LIB}
INC       = -I${WGRIB2_INC}

$(TARGET2): $(OBJECTS2)
	$(FC) $(F_FLAGS) -o $@ $(INC) $(OBJECTS2) $(LIBS)

clean: 
	rm -f *.o 
# install:
	# -cp $(TARGET2) ../../../bin/

clobber: clean
	-rm -f $(TARGET2)
